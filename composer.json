{
    "name": "pbald/postgres-search-bundle",
    "type": "symfony-bundle",
    "description": "Tools to use full-text search PostgreSQL in Doctrine.",
    "keywords": ["doctrine2", "symfony2", "search", "postgresql"],
    "homepage": "https://github.com/pbald/postgres-search-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gruzd Sergey",
            "homepage": "https://github.com/ddmaster"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/orm": ">=2.2.3"
    },
    "autoload": {
        "psr-4": { "PBald\\PostgreSearchBundle": "lib/" }
    },
    "autoload-dev": {
        "psr-4": {
            "PBald\\SPgSp\\PostgreSearchBundle\\": "tests/"
        }
    }
}
